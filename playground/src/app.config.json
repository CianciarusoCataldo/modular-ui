{
  "APP_NAME": "Modular-ui",
  "FALLBACK_LANGUAGE": "en",
  "ROUTER": {
    "BASENAME": "/modular-ui",
    "PAGES": {
      "Home": {
        "path": "/"
      },
      "Button": { "path": "/components/atoms/Button" },
      "Card": { "path": "/components/molecules/Card" },
      "Carousel": { "path": "/components/molecules/Carousel" },
      "CheckBox": { "path": "/components/atoms/CheckBox" },
      "CodeBox": { "path": "/components/atoms/CodeBox" },
      "Counter": { "path": "/components/molecules/Counter" },
      "Divider": { "path": "/components/atoms/Divider" },
      "Dropdown": { "path": "/components/molecules/Dropdown" },
      "Form": { "path": "/components/molecules/Form" },
      "Input": { "path": "/components/atoms/Input" },
      "Link": { "path": "/components/atoms/Link" },
      "Modal": { "path": "/components/atoms/Modal" },
      "Rater": { "path": "/components/molecules/Rater" },
      "Table": { "path": "/components/molecules/Table" },
      "Toggle": { "path": "/components/atoms/Toggle" }
    }
  },
  "I18N": {
    "FALLBACK_LANGUAGE": "en",
    "SUPPORTED_LANGUAGES": ["en", "it", "es", "fr", "de"],
    "NAMESPACES": ["common", "home"],
    "DEFAULT_NAMESPACE": "common",
    "LOAD_PATH": "/locales/{{lng}}/{{ns}}.json"
  }
}
